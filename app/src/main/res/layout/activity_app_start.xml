<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@mipmap/bg_start"
        android:orientation="vertical">
        <RelativeLayout
            android:paddingTop="@dimen/space_15"
            android:layout_width="match_parent"
            android:layout_height="@dimen/space_60">

            <webhall.tyky.com.wangyangming.wights.CircleTextProgressbar
                app:out_circle_color="@color/white"
                android:id="@+id/timer"
                android:layout_width="@dimen/space_30"
                android:layout_height="@dimen/space_30"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/space_15"
                android:layout_marginTop="@dimen/space_5"
                android:clickable="true"
                android:text="跳过"
                android:textColor="@color/white"
                android:textSize="@dimen/text_size_12"
                app:in_circle_color="@color/appstar_timer_select" />

        </RelativeLayout>



    </LinearLayout>
</layout>